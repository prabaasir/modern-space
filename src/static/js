$(document).ready(function () {
  jQuery(function () {
    jQuery('.show').click(function () {
      console.log('sfs')
      jQuery('.targetDiv').hide();
      jQuery('#div' + $(this).attr('target')).show();
    });
  });
})

$(document).ready(function () {
  $('.dd .ad-link').click(function () {
    $('.ad-link').removeClass("ad-active");
    $(this).addClass("ad-active");
  });
});
